{
  "multipart": [
    {
      "apply": {
        "model": "elementalcraft:block/elementpipe_core"
      }
    },
    {
      "when": {
        "cover": "frame"
      },
      "apply": {
        "model": "elementalcraft:block/cover_frame",
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "x": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "connected|extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_side",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "y": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "x": 270,
        "uvlock": true
      }
    },
    {
      "when": {
        "down": "extract"
      },
      "apply": {
        "model": "elementalcraft:block/elementpipe_extract",
        "x": 90,
        "uvlock": true
      }
    }
  ]
}